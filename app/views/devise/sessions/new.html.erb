<div id="page_title">
	<div id="page_title_content">
		<span class="title_black">Anmelden</span>
	</div>
	<div id="page_title_actions">
		<div id="page_title_actions_container">
			<div class="actions_110"> 
				<span><%= link_to_function 'Anmelden', "login.submit()", :class => "link_white" %></span>
			</div> 
			<div class="actions_130"> 
				<span><%= link_to 'Registrieren', new_registration_path(resource_name), :class => "link_white" %></span>
			</div> 
			<div class="actions_170"> 
				<span><%= link_to 'Passwort vergessen', new_password_path(resource_name), :class => "link_white" %></span>
			</div> 
		</div>
	</div> 
</div>
<div id="page_text">
	<%= render 'form' %>
</div>
